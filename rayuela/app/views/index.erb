</header>
<h1 id="main-title">Â¡RAYUELA!</h1>
<p id="counter"><p>
<p id="winner"><p>
<button id="start">Play</button>
<table class="table">
  <tr id="Player1">
    <td id="player1"><strong>Player 1</strong></td>
    <td id="0"  class="active first"></td>
    <%60.times do |num| %>
      <%if num == 50 %>
        <td id="<%= num+1%>" class="red"></td>
      <%else%>
        <td id="<%= num+1 %>"></td>
      <%end%>
    <%end%>
  </tr>
  <tr id="Player2">
    <td id="player2"><strong>Player 2</strong></td>
    <td id="0" class="active first"></td>
    <%60.times do |num|%>
      <%if num == 50 %>
        <td id="<%= num+1%>" class="red"></td>
      <%else%>
        <td id="<%= num+1%>"></td>
      <%end%>
    <%end%>
  </tr>
</table>